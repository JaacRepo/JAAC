# Gnuplot script file for plotting data in file "force.dat"
# This file is called   lot_script
set key font "Times-Roman,10"
set key tmargin
set   autoscale                        # scale axes automatically
unset log                               # remove any log-scaling
unset label                            # remove any previous labels
#set ytic auto                          # set ytics automatically
#set logscale x
set xlabel "Fibbonachi Number (2^n Actors)" font 'Times-Roman,10'
set ylabel "Time (seconds)" font 'Times-Roman,10'
set xr [24:31]
set yr [0:70] 
#set datafile separator ","
set terminal pdf
#set xtics offset 0,0.5
#set xtics ('0' 6, '1' 7, '2' 8, '4' 9, '8' 10, '16' 11)
set output 'fib.pdf'
plot "macfib.txt" using 1:2 title 'Scala Actors' with linespoints lt rgb "red",\
 "macfib.txt" using 1:3 title 'Akka Actors' with linespoints lt rgb "blue",\
 "macfib.txt" using 1:4 title 'JAAC Actors' with linespoints lt rgb "green"
set output
